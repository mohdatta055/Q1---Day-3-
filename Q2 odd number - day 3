ublic class Main {
    public static void main(String[] args) {
        System.out.println("Hello world!");
        //  Q2:print from 1 to 100 and print only odd number 
        for (int i = 1; i <= 100; i ++) {
            // if  the number is even
            if (i % 2 == 0) {
                // skip the even number
                continue;
            }
            // print only odd number 
            System.out.println(i);
        }


        }
    }
